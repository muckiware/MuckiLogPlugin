{
	"name" : "muckiware/log-plugin",
	"description" : "Mucki Log Plugin",
	"version": "v2.2.0",
	"type" : "shopware-platform-plugin",
	"license" : "MIT",
	"authors": [
		{
			"name": "muckiware"
		}
	],
	"extra" : {
		"shopware-plugin-class" : "MuckiLogPlugin\\MuckiLogPlugin",
		"copyright" : "(c) by muckiware",
		"label" : {
			"de-DE" : "Mucki Log Erweiterung",
			"en-GB" : "Mucki Log Plugin"
		}
	},
	"require" : {
		"php" : ">= 8.1"
	},
	"require-dev" : {
		"phpunit/phpunit" : "~8.3.4",
		"phpstan/phpstan" : "^0.12.18",
		"shopware/core" : "*",
		"shopware/storefront" : "*"
	},
	"autoload" : {
		"psr-4" : {
			"MuckiLogPlugin\\" : "src/"
		}
	},
	"config": {
		"allow-plugins": {
			"symfony/runtime": true,
			"php-http/discovery": false
		}
	}
}